{
  "name": "sto-areas",
  "description": "Area of Study data for St. Olaf college",
  "version": "3.0.0-beta.0",
  "main": "dist/index.js",
  "repository": "hawkrives/gobbldygook-area-data",
  "author": "Hawken M. Rives",
  "license": "MIT",
  "scripts": {
    "update-peg": "pegjs < static-areas/bin/hanson-peg.pegjs | babel > static-areas/bin/hanson-peg.js",
    "try": "babel-node static-areas/bin/hanson-to-js.js static-areas/area-files/majors/studio-art.hanson.yaml",
    "push": "./bin/publish.sh",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "build": "babel src/ --out-dir dist/",
    "clean": "rm -rf dist/",
    "mocha": "mocha",
    "eslint": "eslint src/",
    "test": "npm run mocha && npm run eslint",
    "watch-tests": "nodemon --watch test/ --exec 'npm run mocha'"
  },
  "devDependencies": {
    "babel": "5.6.14",
    "babel-eslint": "3.1.20",
    "bluebird": "2.9.32",
    "chai": "3.0.0",
    "chai-as-promised": "5.1.0",
    "eslint": "0.24.0",
    "graceful-fs": "4.1.2",
    "js-yaml": "3.3.1",
    "lodash": "3.10.0",
    "mocha": "2.2.5",
    "nodemon": "1.3.7",
    "pegjs": "0.8.0"
  },
  "keywords": [
    "stolaf",
    "sto-scheduler"
  ]
}
