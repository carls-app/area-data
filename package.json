{
  "name": "sto-areas",
  "description": "Area of Study data for St. Olaf college",
  "version": "3.0.0-beta.0",
  "main": "dist/index.js",
  "repository": "hawkrives/gobbldygook-area-data",
  "author": "Hawken M. Rives",
  "scripts": {
    "push": "./bin/publish.sh",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "build": "babel src/ --out-dir dist/",
    "clean": "rm -rf dist/",
    "mocha": "mocha",
    "eslint": "eslint src/",
    "test": "npm run mocha && npm run eslint",
    "watch-tests": "nodemon --watch test/ --exec 'npm run mocha'"
  },
  "dependencies": {
    "lodash": "^3.7.0"
  },
  "devDependencies": {
    "babel": "5.1.13",
    "babel-eslint": "3.0.1",
    "chai": "2.3.0",
    "chai-as-promised": "5.0.0",
    "eslint": "0.20.0",
    "mocha": "2.2.4",
    "nodemon": "1.3.7"
  },
  "keywords": [
    "stolaf",
    "sto-scheduler"
  ]
}
