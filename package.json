{
  "name": "sto-areas",
  "description": "Area of Study data for St. Olaf college",
  "version": "2.3.9",
  "main": "dist/index.js",
  "repository": "hawkrives/gobbldygook-area-data",
  "author": "Hawken M. Rives",
  "scripts": {
    "push": "./bin/publish.sh",
    "prepublish": "./bin/compile.sh",
    "postpublish": "npm run clean",
    "clean": "rm -rf dist/",
    "deep-clean": "rm -rf dist/ node_modules/",
    "test-function": "mocha",
    "test-style": "jscs ./",
    "test": "npm run test-function && npm run test-style",
    "watch-tests": "nodemon --watch test/ --exec 'npm run test-function'"
  },
  "dependencies": {
    "lodash": "^3.0.0",
    "sto-helpers": "^3.1.1"
  },
  "devDependencies": {
    "6to5": "^2.13.7",
    "chai": "^1.10.0",
    "chai-as-promised": "^4.1.1",
    "jscs": "^1.10.0",
    "jscs-jsdoc": "^0.4.4",
    "mocha": "^2.1.0",
    "nodemon": "^1.3.2"
  },
  "keywords": [
    "stolaf",
    "sto-scheduler"
  ]
}
