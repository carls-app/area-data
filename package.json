  "author": "Hawken M. Rives",
  "license": "MIT",
  "scripts": {
    "build": "pegjs < lib/parse-hanson-string.pegjs | babel --compact 'true' > lib/parse-hanson-string.js",
    "cover": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "coveralls": "npm run cover && coveralls < ./coverage/lcov.info",
    "eslint": "eslint lib/ bin/",
    "mocha": "mocha",
    "precover": "npm run eslint",
    "pretest": "npm run eslint",
    "test": "npm run mocha",
    "travis": "npm run coveralls"
  },
  "dependencies": {
    "commander": "2.8.1",
    "graceful-fs": "4.1.2",
    "humanize-list": "1.0.0",
    "js-yaml": "3.3.1",
    "lodash": "3.10.0"
  },
  "devDependencies": {
    "babel": "5.6.14",
    "babel-eslint": "3.1.21",
    "chai": "3.0.0",
    "coveralls": "2.11.2",
    "eslint": "0.24.0",
    "isparta": "3.0.3",
    "mocha": "2.2.5",
    "pegjs": "0.8.0",
  },
  "keywords": [
    "sto-scheduler",
    "stolaf"
  ]
