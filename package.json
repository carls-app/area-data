{
  "name": "sto-areas",
  "description": "Area of Study data for St. Olaf college",
  "version": "3.0.0-beta.1",
  "repository": "hawkrives/gobbldygook-area-data",
  "author": "Hawken M. Rives",
  "license": "MIT",
  "config": {
    "beautify": "-f - --end-with-newline --indent-size 2"
  },
  "scripts": {
    "benchmark": "babel-node ./benchmarks/students-benchmark.js --runs 50 --graph",
    "build": "npm run build:peg && npm run build:areas",
    "build:areas": "./bin/compile-areas areas/ --out-dir dist/",
    "build:peg": "pegjs < lib/parse-hanson-string.pegjs | babel --compact 'true' | js-beautify $npm_package_config_beautify > lib/parse-hanson-string.js",
    "cosmos": "cd cosmos && component-playground",
    "cover": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "cover:view": "npm run cover && open coverage/lcov-report/index.html",
    "coveralls": "npm run cover && coveralls < ./coverage/lcov.info && codeclimate-test-reporter < ./coverage/lcov.info",
    "eslint": "eslint benchmarks/ bin/ lib/ test/",
    "mocha": "mocha",
    "mocha:slow": "mocha -R mocha-slow-reporter",
    "precover": "npm run eslint",
    "pretest": "npm run eslint",
    "profile": "cat profile/README.md",
    "test": "npm run mocha",
    "test:students": "mocha ./bin/test-students",
    "travis": "npm run test:students && npm run coveralls"
  },
  "dependencies": {
    "graceful-fs": "4.1.2",
    "humanize-list": "1.0.0",
    "js-yaml": "3.3.1",
    "json-stable-stringify": "1.0.0",
    "junk": "1.0.2",
    "lodash": "3.10.0",
    "meow": "3.3.0"
  },
  "devDependencies": {
    "babel": "5.6.23",
    "babel-eslint": "3.1.23",
    "chai": "3.2.0",
    "classnames": "2.1.3",
    "codeclimate-test-reporter": "0.1.0",
    "cosmos-js": "0.6.6",
    "coveralls": "2.11.3",
    "eslint": "0.24.1",
    "isparta": "3.0.3",
    "js-beautify": "1.5.10",
    "mkdirp": "0.5.1",
    "mocha": "2.2.5",
    "mocha-slow-reporter": "0.0.0",
    "node-libs-browser": "0.5.2",
    "opn-cli": "1.0.0",
    "pegjs": "0.8.0",
    "pretty-ms": "1.4.0",
    "react": "0.13.3",
    "sparkly": "2.0.1"
  },
  "keywords": [
    "sto-scheduler",
    "stolaf"
  ]
}
