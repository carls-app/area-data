{
  "name": "sto-areas",
  "description": "Area of Study data for St. Olaf college",
  "version": "1.0.3",
  "main": "index.js",
  "repository": "hawkrives/gobbldygook-area-data",
  "scripts": {
    "push": "./bin/publish.sh",
    "test-function": "mocha",
    "test-style": "jscs src/",
